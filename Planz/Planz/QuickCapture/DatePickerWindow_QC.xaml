<Window x:Class="QuickCapture.DatePickerWindow_QC"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="DatePickerWindow" Height="229" Width="198" xmlns:my="http://schemas.microsoft.com/wpf/2008/toolkit" Loaded="Window_Loaded" Name="DatePicker_QC" ShowInTaskbar="False">
    <Grid>
        <my:Calendar Margin="0,-2,0,2" Name="cal" SelectedDatesChanged="cal_SelectedDatesChanged" />
        <Button Height="24" HorizontalAlignment="Left" Margin="22,165,0,0" Name="ok" VerticalAlignment="Top" Width="48" Click="OK_Click">OK</Button>
        <Button Height="24" HorizontalAlignment="Right" Margin="0,0,22,2" Name="cancel" VerticalAlignment="Bottom" Width="48" Click="Cancel_Click">Cancel</Button>
    </Grid>
</Window>
