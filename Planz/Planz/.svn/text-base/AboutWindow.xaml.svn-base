<Window x:Class="Planz.AboutWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="About Planz(TM)" Height="180" Width="300" ResizeMode="NoResize" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Margin="10, 5, 0, 0">
                <TextBlock Text="Planz(TM) 8.2 Build r1504 6/11/2010" Name="TextBlock_Build"/>
                <LineBreak />
                <LineBreak />
                <Hyperlink NavigateUri="http://kftf.ischool.washington.edu/index.htm" RequestNavigate="Hyperlink_RequestNavigate">Keeping Found Things Found</Hyperlink>
                <LineBreak />
                <TextBlock Text="Information School, University of Washington" />
                <LineBreak />
                <LineBreak />
                <Hyperlink NavigateUri="mailto:kftf@uw.edu" RequestNavigate="Email_RequestNavigate">Email: kftf@uw.edu</Hyperlink>
                <LineBreak />
            </TextBlock>
            <Button Width="80" Content="OK" IsDefault="True" Click="Button_Click" />
        </StackPanel>
    </Grid>
</Window>
