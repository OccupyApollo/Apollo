<Window x:Class="Planz.DeleteWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="" Height="130" Width="300" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock x:Name="Message" Text="" FontFamily="Arial" FontSize="12" Height="40" Margin="10, 10, 10, 0" TextWrapping="WrapWithOverflow" />
            <StackPanel Orientation="Horizontal" Margin="0, 10, 10, 0" HorizontalAlignment="Right" >
                <Button x:Name="BtnYes" Content="YES" Width="60" Height="25" Margin="0, 0, 5, 0" Click="YES_Click" />
                <Button x:Name="BtnNo" Content="NO" Width="60" Height="25" Click="NO_Click" IsCancel="True" IsDefault="True" />
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
